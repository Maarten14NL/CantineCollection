networks:
  collectionapp-network:
    driver: bridge
    external: true
    name: collectionapp-network
services:
  collectionadmin:
    build:
      context: C:\fontys\git\CantineCollection\backend\CollectionApp
      dockerfile: ../CollectionAdmin/CollectionAdmin/Dockerfile
    container_name: collectionadmin
    image: collectionapp
    networks:
      collectionapp-network: {}
  collectionapp:
    build:
      context: C:\fontys\git\CantineCollection\backend\CollectionApp
      dockerfile: CollectionApp/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CollectionApp
      target: base
    container_name: collectionapp
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: collectionapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/CollectionApp.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      collectionapp-network: {}
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\fontys\git\CantineCollection\backend\CollectionApp\CollectionApp:/app:rw
    - C:\Users\maart\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\maart\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\maart\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\maart\.nuget\packages:/root/.nuget/packages:ro
    - C:\fontys\git\CantineCollection\backend\CollectionApp:/src:rw
  db-server:
    container_name: db-server
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: G4sLOj7P
    image: mcr.microsoft.com/mssql/server
    networks:
      collectionapp-network: {}
    ports:
    - published: 1400
      target: 1433
version: '3.4'